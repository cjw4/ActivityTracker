<!DOCTYPE html>
<html>
  <head>
    <title>Activity Tracker</title>
		<%= render 'layouts/head' %>
	</head>
		<%= render 'layouts/header' %>
		<body>
			<div class="col-md-8 col-md-offset-2">
				<% flash.each do |message_type, message| %>
					<div class="alert alert-<%= message_type %>"><%= message %></div>
				<% end %>      			
				<%= yield %>
			</div>
		</body>
</html>
